{
  "name": "express-authorshaven",
  "version": "1.0.0",
  "description": "A Social platform for the creative at heart",
  "main": "index.js",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "linter": "./node_modules/eslint/bin/eslint.js './**/*.js'",
    "start": "npm run build && npm run serve",
    "serve": "node ./dist/index.js",
    "postinstall": "npm run db:migrate",
    "test": "export NODE_ENV=test && npm run db:reset && npm run db:migrate && nyc --reporter=text --reporter=lcov mocha ./tests --require babel-polyfill --require babel-register --exit --recursive",
    "build": "rm -rf dist/ && babel ./server --out-dir dist/ --ignore ./node_modules",
    "start:dev": "node_modules/nodemon/bin/nodemon.js --exec babel-node server/index.js",
    "db:migrate": "node_modules/.bin/sequelize db:migrate && npm run db:seed:all",
    "db:reset": "node_modules/.bin/sequelize db:migrate:undo:all",
    "db:seed:all": "node_modules/.bin/sequelize  db:seed:all",
    "db:seed-reset": "node_modules/.bin/sequelize  db:seed:undo:all"
  },
  "engines": {
    "node": "10.13.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run linter"
    }
  },
  "author": "Andela Simulations Programme",
  "license": "MIT",
  "dependencies": {
    "@sendgrid/mail": "^6.3.1",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "express-session": "^1.15.6",
    "faker": "^4.1.0",
    "jsonwebtoken": "^8.3.0",
    "method-override": "^2.3.10",
    "methods": "^1.1.2",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth": "^1.0.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "pg": "^7.6.1",
    "pg-hstore": "^2.3.2",
    "request": "^2.87.0",
    "rosie": "^2.0.1",
    "sequelize": "^4.41.2",
    "sequelize-cli": "^5.4.0",
    "sequelize-test-helpers": "^1.0.6",
    "slug": "^0.9.3",
    "swagger-jsdoc": "^3.2.6",
    "swagger-ui-express": "^4.0.2",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/node": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "chai-passport-strategy": "^1.0.1",
    "coveralls": "^3.0.2",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.2.0",
    "mocha": "^5.2.0",
    "nock": "^10.0.3",
    "nodemon": "^1.18.7",
    "nyc": "^13.1.0",
    "sequelize-test-helpers": "^1.0.6",
    "sinon": "^7.1.1",
    "sinon-chai": "^3.3.0"
  }
}
